{
	"name": "library-antdesign",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"biome:lint": "biome lint ./src",
		"biome:check": "biome check --apply ./src",
		"biome:format": "biome format ./src",
		"biome:format:write": "biome format --write ./src"
	},
	"dependencies": {
		"antd": "^5.16.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.6.3",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	}
}
